<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="Магазин товаров" Height="650" Width="900">
    <Grid>
        <StackPanel>
            <TextBlock TextWrapping="Wrap" Text="Начальное значение:" Width="200"/>
            <TextBox x:Name="StartTextBox" TextWrapping="Wrap" Width="200"/>
            <TextBlock TextWrapping="Wrap" Text="Конечное значение:" Width="200" Margin="0,10,0,0"/>
            <TextBox x:Name="EndTextBox" TextWrapping="Wrap" Width="200"/>
            <Button Content="Сгенерировать" Click="GenerateButtonClick" Margin="0,10,0,0" Width="200"/>
            <ProgressBar x:Name="ProgressBar" Height="20" Width="200"/>
            <ListBox x:Name="NumbersListBox" Height="100" Width="500" Margin="0,10,0,0"/>
        </StackPanel>
    </Grid>
</Window>
